{
  "name": "dazzler",
  "version": "0.0.1",
  "description": "Reactive web python.",
  "main": "assets/dist/dazzler.js",
  "module": "assets/src/renderer/js/index.js",
  "scripts": {
    "test": "npm run format:test && npm run lint",
    "build:webpack": "npx webpack --config webpack.config.js",
    "build:dev": "npm run build:webpack -- --mode=development",
    "build:meta::core": "npx react-docgen assets/src/core/js/components --pretty > assets/dist/dazzler_core_meta.json",
    "build:meta::test": "npx react-docgen assets/src/internal/test_components/components --pretty > assets/dist/dazzler_test_meta.json",
    "build:meta::extra": "npx react-docgen assets/src/extra/js/components --pretty > assets/dist/dazzler_extra_meta.json",
    "build:meta::dates": "npx react-docgen assets/src/dates/js/components --pretty > assets/dist/dazzler_dates_meta.json",
    "build:meta": "npm run build:meta::core && npm run build:meta::test && npm run build:meta::extra && npm run build:meta::dates",
    "build:dazzler::core": "dazzler generate assets/dist/dazzler_core_meta.json dazzler/components/core",
    "build:dazzler::test": "dazzler generate assets/dist/dazzler_test_meta.json tests/components/spec_components",
    "build:dazzler::extra": "dazzler generate assets/dist/dazzler_extra_meta.json dazzler/components/extra",
    "build:dazzler::dates": "dazzler generate assets/dist/dazzler_dates_meta.json dazzler/components/dates",
    "build:dazzler": "npm run build:dazzler::core && npm run build:dazzler::test && npm run build:dazzler::extra && npm run build:dazzler::dates",
    "build": "npm run build:webpack && npm run build:meta && npm run build:dev && npm run build:dazzler",
    "watch": "npm run build:dev -- --watch",
    "lint": "eslint assets/src",
    "format": "prettier --write assets/src/**/*.js assets/src/**/*.jsx",
    "format:test": "prettier assets/src/**/*.js assets/src/**/*.jsx --list-different"
  },
  "author": "Philippe Duval",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-react": "^7.13.0",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "prop-types": "^15.7.2",
    "react-docgen": "^4.1.1",
    "react-docgen-loader": "^2.9.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-bundle-tracker": "^0.4.2-beta",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "highlight.js": "^9.15.8",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dates": "^20.2.4",
    "react-dom": "^16.8.6",
    "remarkable": "^1.7.1"
  },
  "peerDependencies": {
    "react": "^15.6.0 || ^16.0.0",
    "react-dom": "^15.6.0 || ^16.0.0"
  }
}
