{
  "name": "dazzler",
  "version": "0.0.1",
  "description": "Async Web Framework",
  "main": "assets/dist/dazzler.js",
  "module": "src/renderer/js/index.js",
  "scripts": {
    "test": "npm run format:test && npm run lint",
    "build:webpack": "webpack --config webpack.config.js",
    "build:dev": "npm run build:webpack -- --mode=development",
    "build:meta": "webpack --config webpack.meta.config.js",
    "build:electron": "webpack --config webpack.electron-main.config.js",
    "build:electron:components": "webpack --config webpack.electron-renderer.config.js",
    "build:electron:components::dev": "npm run build:electron:components -- --mode=development",
    "build:dazzler::core": "dazzler generate src/core/js/components dazzler/components/core --ts",
    "build:dazzler::test": "dazzler generate src/internal/test_components/components tests/components/spec_components",
    "build:dazzler::ts": "dazzler generate src/internal/ts_components/components tests/components/ts_components --ts",
    "build:dazzler::extra": "dazzler generate src/extra/js/components dazzler/components/extra --ts",
    "build:dazzler::calendar": "dazzler generate src/calendar/js/components dazzler/components/calendar --ts",
    "build:dazzler::markdown": "dazzler generate src/markdown/js/components dazzler/components/markdown",
    "build:dazzler::auth": "dazzler generate src/auth/js/components dazzler/components/auth --ts",
    "build:dazzler::icons": "dazzler generate src/icons/js/components dazzler/components/icons",
    "build:dazzler::electron": "dazzler generate src/electron/renderer/components dazzler/components/electron --ts",
    "build:dazzler": "run-p build:dazzler::core build:dazzler::test build:dazzler::extra build:dazzler::calendar build:dazzler::markdown build:dazzler::auth build:dazzler::icons build:dazzler::ts build:dazzler::electron",
    "build:docs": "docs/build.sh",
    "build": "run-s build:webpack build:dev build:electron build:electron:components build:electron:components::dev",
    "watch": "npm run build:dev -- --watch",
    "lint": "eslint src --ext js --ext jsx --ext ts --ext tsx",
    "format": "prettier --write ./src/**/*.js src/**/*.jsx src/**/*.ts src/**/*.tsx",
    "format:test": "prettier ./src/**/*.js src/**/*.jsx src/**/*.ts src/**/*.tsx --list-different",
    "mypy": "mypy dazzler tests",
    "electron": "electron ./dazzler/assets/electron-dazzler.js"
  },
  "author": "Philippe Duval",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@types/ramda": "^0.27.44",
    "@types/react": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^6.2.0",
    "electron": "^14.0.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prop-types": "^15.7.2",
    "react-docgen": "^5.4.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.1.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "dotenv": "^10.0.0",
    "prism-react-renderer": "^0.1.7",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-colorful": "^5.3.0",
    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
